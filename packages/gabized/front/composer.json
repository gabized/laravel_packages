{
    "name": "gabized/front",
    "type": "library",
    "description": ":frontend package for demo site.",
    "keywords": [
        "gabized",
        "front"
    ],
    "homepage": "https://bitbucket.org/gabized/larademo",
    "license": "MIT",
    "authors": [
        {
            "name": ":gabized",
            "email": ":gabibwm@gmail.com",
            "homepage": ":https://gabized.com",
            "role": "Developer"
        }
    ],
    "require": {
        "illuminate/support": "~5.1",
        "php" : "~5.6|~7.0"
    },
    "require-dev": {
        "phpunit/phpunit" : ">=5.4.3",
        "squizlabs/php_codesniffer": "^2.3"
    },
    "autoload": {
        "psr-4": {
            "gabized\\front\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "gabized\\front\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs -p --standard=PSR2 --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 src tests",
        "fix-style": "phpcbf -p --standard=PSR2 --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 src tests"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
